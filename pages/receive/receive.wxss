page{
  height: 100%;
  width: 100%;
  margin: 0rpx;
  padding: 0rpx;
}
#background{
  width: 100%;
  height: 100%;
  background-color: rgba(238,229,224);
  position: absolute;
  z-index: -1;
  display: flex;
  flex-direction: column;
}
.commpoent{
  width: 100%;
  height: 30%;
}
#commopent_img{
  position: absolute;
  z-index: -1;
  width: 100%;
  height: 30%;
}
#title{
  writing-mode: vertical-rl;
  width: 13%;
  height: 70%;
  color: grey;
  padding: 0;
  margin-top: 15%;
  font-size: 100%;
}
#main{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: row;
}
#fuck{
  display: flex;
  flex-direction: column;
  height: 100%;
  width: 50%;
}
#main_img{
  position: relative;
  top: 10%;
  height: 70%;
  width: 100%;
  border-radius: 10%;
  margin-left: 2%;
}
#date{
  position: relative;
  margin-top: 14%;
  width: 100%;
  height: 10%;
  font-size: 50%;
  color: orange;
}
#fake{
  width: 37%;
}
#logo{
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 30%;
}
#logo_img{
  height: 100%;
  width: 40%;
}
#fake_img{
  margin-top: 10%;
  height: 90%;
  width: 40%;
}
#details{
  width: 100%;
  height: 70%;
}
#details_sentences{
  position: relative;
  left: 30%;
  word-wrap: normal;
  width: 40%;
  height: 60%;
  font-size: 80%;
  color: grey;
}
#use_button{
  justify-content: center;
  text-align: center;
  height: 20%;
  width: 40%;
  font-size: 100%;
  position: relative;
  left: 35%;
  color: snow;
  background-color: rgba(234,149,24);
  border-radius: 15%;
}